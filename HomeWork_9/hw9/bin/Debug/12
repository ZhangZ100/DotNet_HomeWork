    <script type="text/javascript">

        function replayGif(dom){
            var temp = Q(dom).attr("src");
            dom.src = Q(dom).attr("data-src");
            console.log("play")
            setTimeout((function(dom, temp) {
                return function(){dom.src = temp}
            }(dom, temp)), 200);
            
        }
        if(Q.browser.IE){
            Q(".inner-pic-img").each(function(){
                Q(this).attr("data-src", "false");
                Q(this).attr("src", Q(this).attr("data-default"));
            })
        }
        
        Q(document).ready(function(){
            var nowFocus = 0;
            var canPlayGif = true;
            if(Q(".inner-pic-img").eq(0).attr("data-src") === "false"){
                var canPlayGif = false;
            }
            function refreshNav(){
                var i = -1;
                Q(".main-inner .inner-block").each(function(){
                    if(Q(document).scrollTop()>Q(this).offset().top-140){
                        i++
                    }
                })
                if(i===-1){i=0}
                if(i!==nowFocus){
                    nowFocus = i;
                    Q("#privacy-content-side .link").eq(nowFocus).parent().addClass('selected').siblings().removeClass('selected');
                }
                
                var heightWin = "innerHeight" in window 
                       ? window.innerHeight
                       : document.documentElement.offsetHeight;
                var isPlaying = false
                if(canPlayGif){
                    Q(".inner-pic-img").each(function(){
                        if(Q(document).scrollTop() + heightWin > Q(this).offset().top + 324 && Q(document).scrollTop() < Q(this).offset().top){
                            if(Q(this).attr("data-isplay") === "false"){
                                Q(this).attr("data-isplay", "true")
                                replayGif(this);
                            }
                        }else{
                            if(Q(this).attr("data-isplay") === "true"){
                                Q(this).attr("data-isplay", "false")
                            }
                        }
                    })
                }
               
            }
            Q(document).on("scroll",refreshNav);
            function resizeStyle(){
                var heightWin = "innerHeight" in window
                       ? window.innerHeight
                       : document.documentElement.offsetHeight; 
                if(heightWin < 640){
                    Q(".side-left-fixed").addClass("side-min-height")
                }else{
                    Q(".side-left-fixed").removeClass("side-min-height")
                }
            }
            resizeStyle();
            refreshNav();
            window.onresize = resizeStyle;
        })
        
        Q("#privacy-content-side .link").click(function(){
            var id = Q(this).attr("data-id");
            Q(document).scrollTop(Q("#" + id).offset().top-130);
            Q(this).parent().addClass('selected').siblings().removeClass('selected');
        });
        
    </script>
    


