<body id="j-body" class="qy-1155">
    
        <script type="text/javascript">
    //页面初始化时,给body加class
    (function(){
        var width = document.documentElement.clientWidth;
        var pageBody = document.getElementById('j-body');
        var i, j;
        var defaultResponseInfo = [1155, 1335, 1550, 1765, 1766];
        var responseConfig = window.responseInfo || defaultResponseInfo;
        for (i=0; i<responseConfig.length; i++) {
                j = responseConfig[i];
                if (width <= j) {
                    pageBody.className = 'qy-' + j;
                    return;
                }
        }
        
        pageBody.className = 'qy-' + j;
    })();
</script>         <div class="qy-header" j-toplinefloater="blackFloater" id="block-A">        









 













