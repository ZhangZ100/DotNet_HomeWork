 <script type="text/javascript" charset="utf-8">

    var sdkPackManagerJs = '//static.iqiyi.com/js/sdkpack/sdkpackmanager.js';
    var barrageSdkLoad = false;
    var _ua = navigator.userAgent.toLowerCase();
    var isIEweb = /msie/i.test(_ua);
    function loadJs(url, callback) {   
        var headElem = document.head || document.getElementsByTagName('HEAD').item(0);
        var scriptElem = document.createElement("script");
        scriptElem.type = "text/javascript";
        scriptElem.src = url;
        headElem.appendChild(scriptElem);
        // 异步加载后通过回调来执行
        if (isIEweb) {
            scriptElem.onreadystatechange = function () {
                if (/loaded|complete/.test(scriptElem.readyState)) {
                    callback();
                }
            };
        } else {
            scriptElem.onload = function () {
                callback();
            };
        }
    }
    function _loadloadBarrageSdk(cb) {
        if( !window.__supportPromise__){
            window.postMessage('barrageJSLoaded', window.location.origin);
            return ;
        }
        barrageSdkLoad = true;

        var qyPlayer = window.playerObject;//播放器对象
        var param = {
            id: 'qyBarrageVue',
            player: qyPlayer
        };
        
        var barragePromise  = new Promise( function(r,j){
            window.sdkPackManager.require('barrage', function(sdk) {
                r();
            },{
                business: "1",
                // uri: '//main.iqiyi.com:9190/barrage.js',
                config: {}
            }); 
        });
        var  barrageServerPromise = new Promise( function(r,j){
            window.sdkPackManager.require('barrageServer', function(sdk) {
                r(); 
            },{
                business: "1", 
                config:{}
            });
        })

        Promise.all([ barragePromise  , barrageServerPromise ] ).then( function(){
            console.log('has load. barr barrser')
            window.sdkPackManager.barrage.init(param);
            window.postMessage('barrageJSLoaded', window.location.origin);
            window.vueAppMounted.promise().then(function(){
                var barrageWrap = document.querySelector('#qyBarrageVue');
                var barrageEle = document.querySelector('[data-barrage="qyBarrageVue"]');
                if( barrageEle ){
                    barrageEle.appendChild(barrageWrap);
                    var dom =  document.querySelector('[data-barrage="async"]');
                    dom && dom.remove();
                }
            })
        })
    }
    
    function loadBarrageAsync(player){
        if(barrageSdkLoad) return;
        player.getQiyiPlayerType(function (playerType){
        if(playerType == "h5"){
            if (window.sdkPackManager) {
                _loadloadBarrageSdk();
            } else {
                loadJs(sdkPackManagerJs, function(){
                    _loadloadBarrageSdk();
                });
            }
        } else{
            document.querySelector('[data-barrage="async"]').remove();
        }
        })
    }

    window.QiyiPlayerLoader && window.QiyiPlayerLoader.ready(function loadHandler(playerManager) {
        var player = window.playerObject;
        if (!player) return;
        if (player.eventsRecorder.followUpNextLoad) {
            loadBarrageAsync(player);
        } else {
                        player.on("followUpNextLoad", function (data) {
                loadBarrageAsync(player);
            }, false);
        }
    });
    
</script>        
<script type="text/javascript">
    (function() {
        var _this = this;
        var enableWebp = true;  //默认开启webp替换
        var imgDoms = document.getElementsByClassName('J-sync-webp-replace');

        if (imgDoms && imgDoms.length > 0) {
            for(var i= 0; i < imgDoms.length; i++) {
                var jpgImg = imgDoms[i].getAttribute("data-jpg-img");
                if(_canUseWebp() && enableWebp) {
                    var webpImg = jpgImg.replace(/\.jpg$/, ".webp")
                    imgDoms[i].src = webpImg;

                    //兜底 - 图片响应失败时重新渲染原图片
                    var image = new Image();
                    image.src = webpImg;
                    image.onerror = function() {
                        imgDoms[i].src = jpgImg;
                    }
                } else {
                    imgDoms[i].src = jpgImg;
                }
            }
        }

        function _canUseWebp() {
            return !![].map && document.createElement('canvas').toDataURL('image/webp').indexOf('data:image/webp') === 0;
        }
    }())
</script>        <script>
    var comparator = {
        '<': function (a, b) { return a < b; },
        '<=': function (a, b) { return a <= b; },
        '>': function (a, b) { return a > b; },
        '>=': function (a, b) { return a >= b; }
    };
    function compareVersion(version, range) {
        var string = (range + ''),
            n = +(string.match(/\d+/) || NaN),
            op = string.match(/^[<>]=?|/)[0];
        return comparator[op] ? comparator[op](version, n) : (version == n || n !== n);
    }
    function getUserAgent() {
        var navigator = window.navigator;
        return (navigator && navigator.userAgent || '').toLowerCase();
    }
    function createDeviceCheck(regexp) {
        return function (range) {
            var match = getUserAgent().match(regexp);
            return match !== null && compareVersion(match[1], range);
        }
    }
    function getVendor() {
        var navigator = window.navigator;
        return (navigator && navigator.vendor || '').toLowerCase();
    }

    function isChrome (range) {
        var match = /google inc/i.test(getVendor()) ? getUserAgent().match(/(?:chrome|crios)\/(\d+)/i) : null;
        return match !== null && !isOpera() && compareVersion(match[1], range);
    };
    var arr = [], isOlder = false;
    var isOpera = createDeviceCheck(/(?:^opera.+?version|opr)\/(\d+)/)
    var isEdge = createDeviceCheck(/edge\/(\d+)/)
    var isSafari = createDeviceCheck(/version\/(\d+).+?safari/)
    var isFirefox = createDeviceCheck(/(?:firefox|fxios)\/(\d+)/)
    var isIE = createDeviceCheck(/(?:msie |trident.+?; rv:)(\d+)/);
    //main-site依赖关系 load之后才能加载下一个 不能nomodule和tpye=module同时加载 --xk
    if (isIE() || isFirefox("<60") || isChrome("<61") || isSafari("<=10") || isEdge("<=15") || isOpera("<=47")) {
