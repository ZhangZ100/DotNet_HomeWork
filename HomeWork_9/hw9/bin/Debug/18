            <script>
        var arr = ["//stc.iqiyipic.com/gaze/uniqy/taiwan/js/manifest.a9b29156.js","//stc.iqiyipic.com/gaze/uniqy/taiwan/js/vendor.20ff4821.js","//stc.iqiyipic.com/gaze/uniqy/taiwan/js/common.2bf2f768.js","//stc.iqiyipic.com/gaze/uniqy/taiwan/js/play.eee20ad9.js","//stc.iqiyipic.com/gaze/uniqy/taiwan/js/app.73c5b779.js"];
        var startIdx = 0;
        function loadScriptAsync() {
            if (startIdx > 4) {
                return;
            }
            var dom = document.createElement('script');
            dom.onload = function () {
                startIdx ++;
                loadScriptAsync();
            };
            dom.src = arr[startIdx];
            document.querySelector('body').appendChild(dom);
        }
        var isDevelopEnv = window.location.hostname === 'taiwan.iqiyi.com';
        var loadScriptAsyncOnce = (function () {
            var loaded = false;
            
            return function () {
                if (loaded) return;
                loaded = true;
                loadScriptAsync();
            }
        })();
        setTimeout(loadScriptAsyncOnce, 5000);  
        !isDevelopEnv && window.QiyiPlayerLoader && window.QiyiPlayerLoader.ready(function loadHandler(playerManager) {
            var player = window.playerObject;
            
            if (!player) return;
            if (player.eventsRecorder.followUpNextLoad) {
                loadScriptAsyncOnce();
            } else {
                player.on("followUpNextLoad", function () {
                    loadScriptAsyncOnce()    
                }, false);
            }
              
        });
    </script>
