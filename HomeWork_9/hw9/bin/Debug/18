<script>
    var rightWrap = document.getElementById('twPlayRightWrap');
    var rightElm = document.getElementById('twPlayRightContent');
    var rightTop = document.getElementById('twPlayRightTop');
    var episodesTop = document.getElementById('twPlayEpisodesTop');
    if(rightWrap && rightTop) {
        var heightVal = rightWrap.offsetHeight - rightTop.offsetHeight;
        if(episodesTop) {
            heightVal = heightVal - episodesTop.scrollHeight;
        }
        rightElm.style.height = heightVal + 'px';
        rightElm.style.display = "";
    }
