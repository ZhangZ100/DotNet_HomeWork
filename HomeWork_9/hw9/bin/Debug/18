      s.async = true;
      var qaUrl = document.location.protocol + "//static.iqiyi.com/js/pingback/qa.js";
      if("" != ""){
          qaUrl = document.location.protocol + "//static.iqiyi.com/js/pingback/vipqa.js";
      }
      s.src = qaUrl;
      s.onload = s.onreadystatechange = function () {
        if (!s.readyState || "loaded" == s.readyState || "complete" == s.readyState) {
          if(window.__qlt && window.__qlt.end){
			window.__qlt.end("qaLoadReady");
          }
          s.onload = s.onreadystatechange = null;
        }
      };
      el.parentNode.insertBefore(s, el);
    })();
