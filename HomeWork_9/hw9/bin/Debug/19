    <script>
        (function(){
            var ua = navigator.userAgent.toLowerCase();// 删除地域判断的cookie 
            function delI18n() {
                var date=new Date(); 
                date.setTime(date.getTime()-100000); 
                document.cookie="i18n=0; path=/; domain=iqiyi.com; expires="+date.toGMTString(); 
            }
            if(getCookie("i18n")) {
                delI18n()
            }
            var downloadAppBtn = document.getElementById("header-download-btn");
            if(/(android)|(like mac os x)/i.test(ua)) {
                downloadAppBtn.className = "header-download hide";
            }else{
                downloadAppBtn.className = "header-download"
            }
            function getCookie (key) {
                var reg = new RegExp("(^| )" + key + "=([^;]*)(;|\x24)"),result = reg.exec(document.cookie) || [];
                result = result[2] || null
                if ('string' == typeof result) {return decodeURIComponent(result);} 
                return null;
            };
            var pcUserLoginWrap = document.getElementById('pc-user-login-wrap');
            var pcUserInfoWrap = document.getElementById('pc-user-info-wrap')
            if(!getCookie("P00001")) {
                pcUserLoginWrap.className += " show";
                return;
            }            
            if((getCookie("P00002") && getCookie("P00002") !== "deleted") && (getCookie("P00003") && getCookie("P00003") !== "deleted")) {
                    pcUserInfoWrap.className += " show";
                    var pcUserInfoIcon = document.getElementById('pc-user-info-icon');
                    var pcUserInfoLevel = document.getElementById('pc-user-info-level');
                    var pcUserVipBtn = document.getElementById('pc-user-vip-btn');
                    var cacheUserIcon = getCookie('i18nUserIcon'), cacheVipStatus = getCookie('i18nVipStatus');
                    if(cacheUserIcon) {
                        pcUserInfoIcon.src = cacheUserIcon;
                        if(cacheVipStatus == "1") {
                            pcUserInfoLevel.className = "user-level level-vip-0";
                            pcUserVipBtn.innerHTML = "續費VIP"
                        }
                        else {
                            pcUserInfoLevel.className = "user-level level-0";
                        }
                    }
                    else {
                            document.domain = "iqiyi.com";
    var callbackName = ('jsonp_' + Math.random()).replace(".", "");
    var oHead = document.getElementsByTagName('head')[0];
    var script=document.createElement('script');
    oHead.appendChild(script);
    window[callbackName] = function (json) {
        oHead.removeChild(script);
        clearTimeout(script.timer);
        window[callbackName] = null;
        var data = json.data;
        var iconUrl = data.userInfo.icon;
        if(iconUrl){
            pcUserInfoIcon.src = iconUrl.replace(/^[\w]+:(?=[\/\/])/, '');
        }
        if(data.vipInfo.status == "1") {
            pcUserInfoLevel.className = "user-level level-vip-0";
            pcUserVipBtn.innerHTML = "續費VIP"
        }
        else {
            pcUserInfoLevel.className = "user-level level-0";
        }
        window.uniqy = window.uniqy || {};
        window.uniqy.userData = data;
    };
    script.src = "//pcw-api.iqiyi.com/passport/user/userinfodetail?area=tw&callback=" + callbackName;
                                    }
            } else {
                pcUserLoginWrap.className += " show";
            }
        })()
    </script>
