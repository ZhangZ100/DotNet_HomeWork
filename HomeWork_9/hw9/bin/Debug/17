 <script type="text/javascript" charset="utf-8">

    var sdkPackManagerJs = '//static.iqiyi.com/js/sdkpack/sdkpackmanager.js';
    var barrageSdkLoad = false;
    var _ua = navigator.userAgent.toLowerCase();
    var isIEweb = /msie/i.test(_ua);
    function loadJs(url, callback) {   
        var headElem = document.head || document.getElementsByTagName('HEAD').item(0);
        var scriptElem = document.createElement("script");
        scriptElem.type = "text/javascript";
        scriptElem.src = url;
        headElem.appendChild(scriptElem);
        // 异步加载后通过回调来执行
        if (isIEweb) {
            scriptElem.onreadystatechange = function () {
                if (/loaded|complete/.test(scriptElem.readyState)) {
                    callback();
                }
            };
        } else {
            scriptElem.onload = function () {
                callback();
            };
        }
    }
    function _loadloadBarrageSdk(cb) {
        if( !window.__supportPromise__){
            window.postMessage('barrageJSLoaded', window.location.origin);
            return ;
        }
        barrageSdkLoad = true;

        var qyPlayer = window.playerObject;//播放器对象
        var param = {
            id: 'qyBarrageVue',
            player: qyPlayer
        };
        
        var barragePromise  = new Promise( function(r,j){
            window.sdkPackManager.require('barrage', function(sdk) {
                r();
            },{
                business: "1",
                // uri: '//main.iqiyi.com:9190/barrage.js',
                config: {}
            }); 
        });
        var  barrageServerPromise = new Promise( function(r,j){
            window.sdkPackManager.require('barrageServer', function(sdk) {
                r(); 
            },{
                business: "1", 
                config:{}
            });
        })

        Promise.all([ barragePromise  , barrageServerPromise ] ).then( function(){
            console.log('has load. barr barrser')
            window.sdkPackManager.barrage.init(param);
            window.postMessage('barrageJSLoaded', window.location.origin);
            window.vueAppMounted.promise().then(function(){
                var barrageWrap = document.querySelector('#qyBarrageVue');
                var barrageEle = document.querySelector('[data-barrage="qyBarrageVue"]');
                if( barrageEle ){
                    barrageEle.appendChild(barrageWrap);
                    var dom =  document.querySelector('[data-barrage="async"]');
                    dom && dom.remove();
                }
            })
        })
    }
    
    function loadBarrageAsync(player){
        if(barrageSdkLoad) return;
        player.getQiyiPlayerType(function (playerType){
        if(playerType == "h5"){
            if (window.sdkPackManager) {
                _loadloadBarrageSdk();
            } else {
                loadJs(sdkPackManagerJs, function(){
                    _loadloadBarrageSdk();
                });
            }
        } else{
            document.querySelector('[data-barrage="async"]').remove();
        }
        })
    }

    window.QiyiPlayerLoader && window.QiyiPlayerLoader.ready(function loadHandler(playerManager) {
        var player = window.playerObject;
        if (!player) return;
        if (player.eventsRecorder.followUpNextLoad) {
            loadBarrageAsync(player);
        } else {
                        player.on("followUpNextLoad", function (data) {
                loadBarrageAsync(player);
            }, false);
        }
    });
    
