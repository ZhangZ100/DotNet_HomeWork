    function macVersion() {
        return {
            isMac1014: /Mac OS X 10_14/.test(navigator.appVersion),
            isMac1013: /Mac OS X 10_13/.test(navigator.appVersion),
            isMac1012: /Mac OS X 10_12/.test(navigator.appVersion),
            isMac1011: /Mac OS X 10_11/.test(navigator.appVersion),
            isMac1010: /Mac OS X 10_10/.test(navigator.appVersion),
            isMac1009: /Mac OS X 10_9/.test(navigator.appVersion),
            isMac1008: /Mac OS X 10_8/.test(navigator.appVersion),
            isMac1007: /Mac OS X 10_7/.test(navigator.appVersion),
            isMac1006: /Mac OS X 10_6/.test(navigator.appVersion),
            isMac1005: /Mac OS X 10_5/.test(navigator.appVersion)
        };
    }
    var macVer = this.macVersion();
    window.addEventListener('beforeinstallprompt', function(e) {
    if (macVer.isMac1010 
        || macVer.isMac1009 
        || macVer.isMac1008 
        || macVer.isMac1007 
        || macVer.isMac1006 
        || macVer.isMac1005) {
            return;
        }
        e.preventDefault();
        window.deferredPrompt = e;
        return false;
    });
</script>
