<script type="text/javascript">
    (function() {
        var _this = this;
        var enableWebp = true;  //默认开启webp替换
        var imgDoms = document.getElementsByClassName('J-sync-webp-replace');

        if (imgDoms && imgDoms.length > 0) {
            for(var i= 0; i < imgDoms.length; i++) {
                var jpgImg = imgDoms[i].getAttribute("data-jpg-img");
                if(_canUseWebp() && enableWebp) {
                    var webpImg = jpgImg.replace(/\.jpg$/, ".webp")
                    imgDoms[i].src = webpImg;

                    //兜底 - 图片响应失败时重新渲染原图片
                    var image = new Image();
                    image.src = webpImg;
                    image.onerror = function() {
                        imgDoms[i].src = jpgImg;
                    }
                } else {
                    imgDoms[i].src = jpgImg;
                }
            }
        }

        function _canUseWebp() {
            return !![].map && document.createElement('canvas').toDataURL('image/webp').indexOf('data:image/webp') === 0;
        }
    }())
</script>        <script>
    var comparator = {
        '<': function (a, b) { return a < b; },
        '<=': function (a, b) { return a <= b; },
        '>': function (a, b) { return a > b; },
        '>=': function (a, b) { return a >= b; }
    };
    function compareVersion(version, range) {
        var string = (range + ''),
            n = +(string.match(/\d+/) || NaN),
            op = string.match(/^[<>]=?|/)[0];
        return comparator[op] ? comparator[op](version, n) : (version == n || n !== n);
    }
    function getUserAgent() {
        var navigator = window.navigator;
        return (navigator && navigator.userAgent || '').toLowerCase();
    }
    function createDeviceCheck(regexp) {
        return function (range) {
            var match = getUserAgent().match(regexp);
            return match !== null && compareVersion(match[1], range);
        }
    }
    function getVendor() {
        var navigator = window.navigator;
        return (navigator && navigator.vendor || '').toLowerCase();
    }

    function isChrome (range) {
        var match = /google inc/i.test(getVendor()) ? getUserAgent().match(/(?:chrome|crios)\/(\d+)/i) : null;
        return match !== null && !isOpera() && compareVersion(match[1], range);
    };
    var arr = [], isOlder = false;
    var isOpera = createDeviceCheck(/(?:^opera.+?version|opr)\/(\d+)/)
    var isEdge = createDeviceCheck(/edge\/(\d+)/)
    var isSafari = createDeviceCheck(/version\/(\d+).+?safari/)
    var isFirefox = createDeviceCheck(/(?:firefox|fxios)\/(\d+)/)
    var isIE = createDeviceCheck(/(?:msie |trident.+?; rv:)(\d+)/);
    //main-site依赖关系 load之后才能加载下一个 不能nomodule和tpye=module同时加载 --xk
    if (isIE() || isFirefox("<60") || isChrome("<61") || isSafari("<=10") || isEdge("<=15") || isOpera("<=47")) {
        arr = ["//stc.iqiyipic.com/??gaze/uniqy/main/js/manifest-legacy.359774d1.js,gaze/uniqy/main/js/vendor-legacy.1c3c9446.js","//stc.iqiyipic.com/gaze/uniqy/main/js/common-legacy.12d3ae91.js","//stc.iqiyipic.com/??gaze/uniqy/main/js/playAb-legacy.544059f4.js,gaze/uniqy/main/js/app-legacy.b90bc0fd.js"];
        isOlder = true;
    } else {
        arr = ["//stc.iqiyipic.com/??gaze/uniqy/main/js/manifest.359774d1.js,gaze/uniqy/main/js/vendor.975cb830.js","//stc.iqiyipic.com/gaze/uniqy/main/js/common.ec43f1fb.js","//stc.iqiyipic.com/??gaze/uniqy/main/js/playAb.a18de453.js,gaze/uniqy/main/js/app.b1b1a444.js"];
    }
    var len = arr.length,startIdx = 1;
    var t = '';
    function loadScriptAsync() {
        if (startIdx > len) {
            return;
        }
        var dom = document.createElement('script');
        if (isOlder == false) {
            dom.setAttribute('type','module');
        }
        // dom.setAttribute('crossorigin','use-credentials');
        dom.type="text/javascript";
        dom.onload = function () {
            startIdx ++;
            loadScriptAsync();
        };
        dom.src = arr[startIdx - 1];
        document.querySelector('body').appendChild(dom);
    } 
    var isDevelopEnv = window.location.hostname === 'main.iqiyi.com';
    var loadScriptAsyncOnce = (function (r) {
        var loaded = false;
        return function () {
            if (loaded) return;
            loaded = true;
            window.updateUserIcon();
            loadScriptAsync();
        }
    })();
    if(!isDevelopEnv){
        t = setTimeout(loadScriptAsyncOnce, 6*1000); 
    }
    !isDevelopEnv && window.QiyiPlayerLoader && window.QiyiPlayerLoader.ready(function loadHandler(playerManager) {
        var player = window.playerObject;
        if (!player) return;
        if (player.eventsRecorder.followUpNextLoad) {
            if(t){clearTimeout(t);}
            loadScriptAsyncOnce();
        } else {
                player.on("followUpNextLoad", function (data) {
                if (data && data.data) {
                    var initial = data.data;
